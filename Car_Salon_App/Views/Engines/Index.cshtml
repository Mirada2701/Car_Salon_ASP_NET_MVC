@model List<EngineDto>

<h1>Brand list</h1>

<a asp-action="Create" class="mb-3 btn btn-primary">
    <i class="bi bi-plus-circle-fill"></i>
    Add New Engine
</a>

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Capacity</th>
            <th scope="col">Type</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var e in Model)
        {
            <tr>
                <th scope="row">@e.Id</th>
                <td>@e.Capacity</td>
                <td>@e.Type</td>
            </tr>
        }
    </tbody>
</table>

