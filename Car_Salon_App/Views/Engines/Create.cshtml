@model EngineDto

<h1>Create Engine</h1>


<form asp-action="Create">
    <div class="col-md-6">
        <label asp-for=Capacity class="form-label">Capacity</label>
        <input required asp-for=Capacity type="number" class="form-control" placeholder="Enter capacity engine">
        <span class="small text-danger" asp-validation-for="Capacity"></span>
    </div>
    <div class="col-md-6">
        <label asp-for=Type class="form-label">Type</label>
        <input required asp-for=Type type="text" class="form-control" placeholder="Enter type engine">
        <span class="small text-danger" asp-validation-for="Type"></span>
    </div>
    <button type="submit" class="mt-3 btn btn-primary">Submit</button>
</form>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}